@Article{D2YA00075J,
    author ="Mishra, Snehangshu and Ghosh, Subrata and Boro, Binita and Kumar, Dinesh and Porwal, Shivam and Paul, Mrittika and Dixit, Himanshu and Singh, Trilok",
    title  ="Solution-processed next generation thin film solar cells for indoor light applications",
    journal  ="Energy Adv.",
    year  ="2022",
    volume  ="1",
    issue  ="11",
    pages  ="761-792",
    publisher  ="RSC",
    doi  ="10.1039/D2YA00075J",
    url  ="http://dx.doi.org/10.1039/D2YA00075J",
    abstract  ="Recently{,} indoor photovoltaics have gained research attention due to their potential applications in the Internet of Things (IoT) sector and most of the devices in modern technology are controlled via wireless/or battery-less means and powered by indoor photovoltaics. This review provides an overview of the developments of thin film solar cells{,} particularly solution-processed dye-sensitized solar cells{,} organic solar cells{,} quantum dot solar cells{,} and upcoming organic–inorganic metal halide perovskite solar cells for indoor applications. Further{,} a comprehensive material assessment{,} device design{,} and indoor lighting characteristics are discussed. We also highlight the challenges and prospects for the development of indoor photovoltaics for various IoT applications."
}
